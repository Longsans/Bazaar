{
  "profiles": {
    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "composeLaunchAction": "LaunchBrowser",
      "composeLaunchServiceName": "ordering-api",
      "composeLaunchUrl": "{Scheme}://localhost:{ServicePort}/swagger",
      "composeProfile": {
        "includes": [
          "ordering"
        ],
        "serviceActions": {
          "payment-api": "StartWithoutDebugging",
          "catalog-api": "StartWithoutDebugging"
        }
      }
    }
  }
}